all = c()
for(j in 1:nrow(scores)){

t = checkifknown(scores[j,1],scores[j,2],ksub)

if(t == 1){

known = c(known,as.numeric(scores[j,3]))
}

all = c(all,as.numeric(scores[j,3]))


}
